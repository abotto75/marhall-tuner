{
  "name": "ai-tuner-pro",
  "version": "9.1.0",
  "private": true,
  "scripts": {
    "build:presets": "node scripts/build_presets_from_csv.cjs public/data/presets.csv public/data/presets.json",

    "build:version": "node -e \"const fs=require('fs');const v=require('./package.json').version;fs.writeFileSync('./version.json', JSON.stringify({version:v}))\"",

    "vercel-build": "npm run build:presets && npm run build:version && echo Vercel build step complete"
  },
  "dependencies": {
    "openai": "^4.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
